class Solution {
public:
    vector<int> productExceptSelf(vector<int>& nums) {
        vector<int>vec;
        int p=1;
        for(int i=0;i<nums.size();i++){
           vec.push_back(p);        
            p*=nums[i];
        }
        
        
        int q=1;
        for(int i=nums.size()-1;i>=0;i--){
           vec.at(i)=vec[i]*q;        
            q*=nums[i];
        }
        return vec;
    }
};
