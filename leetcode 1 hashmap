    class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        vector<int> vec;
        unordered_map<int ,int> hash;
        for(int i=0;i<nums.size();i++){
            if(hash.find(target-nums.at(i))!=hash.end()){
                vec.push_back(i);
                vec.push_back(hash.find(target-nums.at(i))->second);
                return vec;
            }
             hash[nums.at(i)]=i;   
            
        }
        return vec;
    }
};
